{
  "title": "POV Cursor Settings",
  "controls": [
    {
      "label": "Show",
      "oscName": "show",
      "type": "toggle",
      "defaultValue": true
    },
    {
      "label": "Height",
      "oscName": "height",
      "type": "slider",
      "min": 0.5,
      "max": 3,
      "step": 0.1,
