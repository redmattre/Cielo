{
  "title": "Speaker Settings",
  "controls": [
    {
      "label": "Level",
      "oscName": "level",
      "type": "slider",
      "min": 0,
      "max": 1,
      "step": 0.05,
      "defaultValue": 1.0
    },
    {
      "label": "Delay",
      "oscName": "delay",
      "type": "slider",
      "min": 0,
      "max": 100,
      "step": 1,
      "defaultValue": 0
    },
    {
      "label": "Mute",
      "oscName": "mute",
      "type": "toggle",
      "defaultValue": false
    },
    {
      "label": "Output",
      "oscName": "output",
      "type": "numbox",
      "min": 1,
      "max": 32,
      "defaultValue": 1
    },
    {
      "label": "EQ High",
      "oscName": "eq_high",
      "type": "slider",
      "min": -12,
      "max": 12,
      "step": 0.5,
      "defaultValue": 0
    },
    {
      "label": "Look at Origin",
      "type": "toggle",
      "defaultValue": true
    }
  ]
}
