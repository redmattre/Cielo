{
  "title": "Speaker Settings",
  "controls": [
    {
      "label": "Trim",
      "oscName": "trim",
      "type": "slider",
      "min": -12.0,
      "max": 6.0,
      "step": 0.01,
      "defaultValue": 0.0,
      "unitType": "dB"
    },
    {
      "label": "Active",
      "oscName": "channelstate",
      "type": "toggle",
      "defaultValue": true
    },
    {
      "label": "Solo",
      "oscName": "solo ",
      "type": "toggle",
      "defaultValue": 0.0
    },
    {
      "type": "button",
      "label": "Test",
      "oscName": "test"
    },
    {
      "label": "Look at Origin",
      "type": "toggle",
      "defaultValue": true
    }
  ]
}
